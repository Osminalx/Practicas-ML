# Agregar el histograma
pl <- pl + geom_histogram()
# Mostrar el gráfico
print(pl)
summary(seguros)
# Leer los datos
seguros <- read.csv("insurance.csv")
# Crear un dataframe para la variable 'Siniestros'
accidentes <- data.frame(Siniestros = seguros$siniestros)
# Crear un dataframe para la variable 'ant_veh'
vehicleAge <- data.frame(ant_veh = seguros$ant_veh)
# Crear el objeto ggplot para 'Siniestros'
pl_accidentes <- ggplot(data = accidentes, aes(x = Siniestros)) +
geom_histogram() +
ggtitle("Histograma de Siniestros")
# Crear el objeto ggplot para 'ant_veh'
pl_vehicleAge <- ggplot(data = vehicleAge, aes(x = ant_veh)) +
geom_histogram() +
ggtitle("Histograma de Antigüedad del Vehículo")
# Combinar los dos gráficos usando facet_wrap
combined_plot <- plot_grid(pl_accidentes, pl_vehicleAge, nrow = 1)
