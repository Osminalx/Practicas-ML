# Leer los datos
seguros <- read.csv("insurance.csv")
# Crear el dataframe
accidentes <- data.frame(Siniestros = seguros$siniestros)
# Verificar si los datos se cargaron correctamente en el dataframe 'accidentes'
head(accidentes)
# Crear el objeto ggplot
